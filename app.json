    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-2617758158623773~1670728833",
      "ios_app_id": "ca-app-pub-2617758158623773~1670728833"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2617758158623773~1670728833",
